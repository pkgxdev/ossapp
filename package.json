    "@types/electron": "^1.6.10",
    "@types/he": "^1.2.0",
    "@types/mousetrap": "^1.6.11",
    "@types/prismjs": "^1.26.0",
    "@vitest/coverage-c8": "^0.27.1",
    "axios": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "buffer": "^6.0.3",
    "canvas-confetti": "^1.6.0",
    "chokidar": "^3.5.3",
    "custom-electron-titlebar": "4.2.0-beta.0",
    "dayjs": "^1.11.7",
    "electron-context-menu": "^3.6.1",
    "electron-log": "^4.4.8",
    "electron-serve": "^1.1.0",
    "electron-updater": "^5.3.0",
    "electron-vite": "^1.0.18",
    "electron-window-state": "^5.0.3",
    "fuse.js": "^6.6.2",
    "he": "^1.2.0",
    "is-what": "^4.1.15",
    "js-yaml": "^4.1.0",
    "koffi": "^2.4.2",
    "libpkgx": "^0.14.0",
    "lodash": "^4.17.21",
    "lorem-ipsum": "^2.0.8",
    "marked": "^5.0.4",
    "mixpanel-browser": "^2.45.0",
    "mkdirp": "^2.1.3",
    "moment": "^2.29.4",
    "mousetrap": "^1.6.5",
    "node-pty": "^1.0.0",
    "outdent": "^0.8.0",
    "prismjs": "^1.29.0",
    "pushy-electron": "^1.0.11",
    "restructured": "0.0.11",
    "svelte-infinite-scroll": "^2.0.1",
    "svelte-markdown": "^0.2.3",
    "svelte-watch-resize": "^1.0.3",
    "sveltekit-i18n": "^2.2.2",
    "undici": "^5.22.1",
    "upath": "^2.0.1",
    "vite-plugin-static-copy": "^0.13.1",
    "xterm": "^5.2.1",
    "xterm-addon-fit": "^0.7.0",
    "yaml": "^2.2.1"
  },
  "homepage": "https://pkgx.app",
  "repository": "https://github.com/pkgxdev/ossapp.git",
  "pkgx": "node^18.16 npm^9.7.2 python@3.11 xc make"
}
